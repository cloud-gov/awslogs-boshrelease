check process awslogs-jammy
  with pidfile /var/vcap/sys/run/awslogs-jammy/awslogs-jammy.pid
  start program "/var/vcap/jobs/awslogs-jammy/bin/monit_debugger awslogs_ctl '/var/vcap/jobs/awslogs-jammy/bin/ctl start'"
  stop program "/var/vcap/jobs/awslogs-jammy/bin/monit_debugger awslogs_ctl '/var/vcap/jobs/awslogs-jammy/bin/ctl stop'"
  group vcap

check process awslogs-scanner
  with pidfile /var/vcap/sys/run/awslogs-jammy/awslogs-scanner.pid
  start program "/var/vcap/jobs/awslogs-jammy/bin/monit_debugger awslogs_scanner_ctl '/var/vcap/jobs/awslogs-jammy/bin/scanner-ctl start'"
  stop program "/var/vcap/jobs/awslogs-jammy/bin/monit_debugger awslogs_scanner_ctl '/var/vcap/jobs/awslogs-jammy/bin/scanner-ctl stop'"
  group vcap
