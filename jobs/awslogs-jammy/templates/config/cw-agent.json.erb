{
  "logfile": "/var/vcap/sys/log/awslogs-jammy/amazon-cloudwatch-agent.log",
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/audit/audit.log",
            "log_group_name": "/var/log/audit/audit.log",
            "log_stream_name": "{instance_id}",
            "retention_in_days": "<%= p("awslogs-jammy.retention-in-days") %>"
          },
          {
            "file_path": "/var/log/auth.log",
            "log_group_name": "/var/log/auth.log",
            "log_stream_name": "{instance_id}",
            "retention_in_days": "<%= p("awslogs-jammy.retention-in-days") %>"
          },
          {
            "file_path": "/var/log/dpkg.log",
            "log_group_name": "/var/log/dpkg.log",
            "log_stream_name": "{instance_id}",
            "retention_in_days": "<%= p("awslogs-jammy.retention-in-days") %>"
          },
          {
            "file_path": "/var/log/syslog",
            "log_group_name": "/var/log/syslog",
            "log_stream_name": "{instance_id}",
            "retention_in_days": "<%= p("awslogs-jammy.retention-in-days") %>"
          }
        ]
      }
    }
  },
  "log_stream_name": "{instance_id}"
}
