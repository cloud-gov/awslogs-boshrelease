check process awslogs-bionic
  with pidfile /var/vcap/sys/run/awslogs-bionic/awslogs-bionic.pid
  start program "/var/vcap/jobs/awslogs-bionic/bin/monit_debugger awslogs_ctl '/var/vcap/jobs/awslogs-bionic/bin/ctl start'"
  stop program "/var/vcap/jobs/awslogs-bionic/bin/monit_debugger awslogs_ctl '/var/vcap/jobs/awslogs-bionic/bin/ctl stop'"
  group vcap

check process awslogs-scanner
  with pidfile /var/vcap/sys/run/awslogs-bionic/awslogs-scanner.pid
  start program "/var/vcap/jobs/awslogs-bionic/bin/monit_debugger awslogs_scanner_ctl '/var/vcap/jobs/awslogs-bionic/bin/scanner-ctl start'"
  stop program "/var/vcap/jobs/awslogs-bionic/bin/monit_debugger awslogs_scanner_ctl '/var/vcap/jobs/awslogs-bionic/bin/scanner-ctl stop'"
  group vcap
